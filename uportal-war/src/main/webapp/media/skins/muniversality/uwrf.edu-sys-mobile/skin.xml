<resources xmlns="http://www.jasig.org/uportal/web/skin"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.jasig.org/uportal/web/skin https://source.jasig.org/schemas/resource-server/skin-configuration/skin-configuration-v1.2.xsd">
  
  <parameter name="fss-theme">fl-theme-mist</parameter>
  
  <!-- Use wisconsin.edu skin as baseline skin. -->
  <css import="true">../wisconsin.edu-sys-mobile/skin.xml</css>
  
  <!-- Campus skin override: Visual style for the portal. -->
  <css>portal.css</css>
  
  <!-- Campus skin override: Local skin override for visual style for portlet content. -->
  <css>portlet.css</css>

  <!-- Campus skin override: Support for legacy uPortal css classes. -->
  <css>legacy.css</css>
    
  <!-- Campus skin override: Optional overrides specific to IE6.  This file needs to be created if you want IE6 specific CSS. -->
  <css conditional="if IE 6">ie6.css</css>
  
  <js import="true">../common/common_skin.xml</js>
</resources>